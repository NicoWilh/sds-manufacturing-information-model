    <owl:ObjectProperty rdf:about="https://open-manufacturing.org/ontologies/I4.0/ProductSegment/isFollowedBy">
        <rdfs:domain rdf:resource="https://open-manufacturing.org/ontologies/I4.0/ProductSegment/ProductSegment"/>
        <rdfs:range rdf:resource="https://open-manufacturing.org/ontologies/I4.0/ProductSegment/ProductSegment"/>
        <rdfs:comment xml:lang="en">Relationship between &quot;ProductSegment&quot; and &quot;ProductSegment&quot;</rdfs:comment>
        <rdfs:label xml:lang="en">is followed by</rdfs:label>
        <rdfs:label>is followed by</rdfs:label>
    </owl:ObjectProperty>
    


