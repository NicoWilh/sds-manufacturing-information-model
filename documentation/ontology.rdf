        <rdfs:comment xml:lang="en">Relationship between &quot;ProductSegment&quot; and &quot;SegmentResult&quot;</rdfs:comment>
        <rdfs:label xml:lang="en">has segment result</rdfs:label>
        <rdfs:label>has segment result</rdfs:label>
    </owl:ObjectProperty>
    


