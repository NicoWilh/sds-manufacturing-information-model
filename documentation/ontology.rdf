        <rdfs:comment xml:lang="en">Relationship between &quot;ProductSegment&quot; and &quot;EquipmentSpecification&quot;</rdfs:comment>
        <rdfs:label xml:lang="en">has equipment specification</rdfs:label>
        <rdfs:label>has equipment specification</rdfs:label>
    </owl:ObjectProperty>
    


