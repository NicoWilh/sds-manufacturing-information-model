        <rdfs:domain rdf:resource="https://open-manufacturing.org/ontologies/I4.0/ProductSegment/ProductSegment"/>
        <rdfs:range rdf:resource="https://open-manufacturing.org/ontologies/I4.0/ProductSegment/MaterialSpecification"/>
        <rdfs:comment xml:lang="en">Relationship between &quot;ProductSegment&quot; and &quot;MaterialSpecification&quot;</rdfs:comment>
        <rdfs:label xml:lang="en">has material specification</rdfs:label>
        <rdfs:label>has material specification</rdfs:label>
