    <owl:ObjectProperty rdf:about="https://open-manufacturing.org/ontologies/I4.0/ProductSegment/hasSegmentResult">
        <rdfs:domain rdf:resource="https://open-manufacturing.org/ontologies/I4.0/ProductSegment/ProductSegment"/>
