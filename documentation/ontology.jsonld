  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Description of a tangible substrate of a specific manufacturing process that is taken in its basic form as input for subsequent manufacturing. It shall not generally be used in the context of tangible input into assembly process. Subclass of material.\n Note 1: It is a perspective on the one who uses it to manufacture other goods – for example a silicon wafer is raw material for production of semi-conductor electronic devices, where by itself it is a result of complex preparation process, hence a product from perspective of its supplier."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Raw Material"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://open-manufacturing.org/ontologies/I4.0/Product/Material"
  } ]
